{
  "name": "@jsdevtools/rehype-inline-svg",
  "version": "1.1.2",
  "description": "A rehype plugin that inlines and optimizes SVG images",
  "keywords": [
    "unified",
    "rehype",
    "plugin",
    "rehype-plugin",
    "inline",
    "optimize",
    "svg",
    "image",
    "img",
    "html"
  ],
  "author": {
    "name": "James Messinger",
    "url": "https://jamesmessinger.com"
  },
  "license": "MIT",
  "homepage": "https://jstools.dev/rehype-inline-svg",
  "repository": {
    "type": "git",
    "url": "https://github.com/JS-DevTools/rehype-inline-svg.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "shx rm -rf .nyc_output coverage lib",
    "lint": "npm run lint:typescript && npm run lint:javascript",
    "lint:typescript": "tslint -p tsconfig.json",
    "lint:javascript": "eslint test",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "mocha && npm run lint",
    "coverage": "nyc node_modules/mocha/bin/mocha",
    "upgrade": "npm-check -u && npm audit fix",
    "bump": "bump --tag --push --all",
    "release": "npm run upgrade && npm run clean && npm run build && npm test && npm run bump"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@jsdevtools/eslint-config-modular": "^8.0.3",
    "@jsdevtools/tslint-modular": "^2.0.5",
    "@jsdevtools/version-bump-prompt": "^6.0.3",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.22",
    "@types/svgo": "^1.3.3",
    "@types/unist": "^2.0.3",
    "@types/vfile": "^4.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.4.0",
    "mocha": "^8.0.1",
    "npm-check": "^5.9.2",
    "nyc": "^15.1.0",
    "rehype-stringify": "^8.0.0",
    "shx": "^0.3.2",
    "to-vfile": "^6.1.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6",
    "typescript-tslint-plugin": "^0.5.5",
    "vfile": "^4.1.1"
  },
  "dependencies": {
    "rehype-parse": "^7.0.1",
    "svgo": "^1.3.2",
    "unified": "^9.0.0"
  }
}
